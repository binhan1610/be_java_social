  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 3
    },
    "password": {
      "type": "string",
      "minLength": 6
    },
    "fcm_token": {
      "type": "string"
    }
  },
  "required": ["username", "password", "fcm_token"]
}
